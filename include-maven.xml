<plugin>
   <groupId>de.axonvisualizer</groupId>
   <artifactId>axonvisualizer-maven-plugin</artifactId>
   <version>0.0.1-SNAPSHOT</version> <!-- This is the version defined in hello-maven-plugin -->
   <executions>
      <execution>
         <phase>compile</phase>
            <goals>
            <goal>generate</goal> <!-- You declared one goal touch in your plugin, not "sayhi" -->
         </goals>
         <configuration>
            <outputFilePath>${project.basedir}/lol.json</outputFilePath>
         </configuration>
      </execution>
   </executions>
</plugin>